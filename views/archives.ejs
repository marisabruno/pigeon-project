<div class="grey-background">
    <div class="global-justify-center">
    <h1>Your Archives</h1>
    </div>

    <div class="global-justify-center">
        <table class="table table-hover">
          <tr>
            <th>Date</th>
            <th>Title</th>
          </tr>
        <%myArchives.forEach((oneArchive)=>{%>
          <tr onclick="window.location='/my-archives/<%=oneArchive._id%>'">
            <td><%=oneArchive.dateAdded.toDateString()%></td>
            <td><%=oneArchive.title%></td>
          </tr>
      <%  }) %>
        </table>
    </div>

</div>
